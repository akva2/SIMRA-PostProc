<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<simulation>

  <!-- General - geometry definitions !-->
  <geometry>
    <patchfile>Box.dat</patchfile>
    <topologysets>
      <set name="Bottom" type="face">
        <item patch="1">5</item>
      </set>
    </topologysets>
  </geometry>

  <fswalldistance>
    <boundaryconditions>
      <dirichlet set="Bottom" comp="-1" type="constant">1</dirichlet>
    </boundaryconditions>
    <wall_value>1</wall_value>
    <nonlinearsolver>
      <dtol>1e3</dtol>
    </nonlinearsolver>
  </fswalldistance>

  <discretization>
    <nGauss>2 2</nGauss>
  </discretization>

  <simra>
    <scaling u="1.0" L="1.0"/>
    <resultfile type="history">Box.res</resultfile>
    <no_stratification/>
  </simra>

  <postprocessing>
    <projection>
      <CGL2 version="2"/>
    </projection>
  </postprocessing>

</simulation>
