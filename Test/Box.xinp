<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<simulation>

  <!-- General - geometry definitions !-->
  <geometry>
    <patchfile>Box.dat</patchfile>
  </geometry>

  <discretization>
    <nGauss>2 2</nGauss>
  </discretization>

  <simra>
    <scaling u="1.0" L="1.0"/>
    <resultfile type="history">Box.res</resultfile>
    <no_stratification/>
  </simra>

  <postprocessing>
    <projection>
      <CGL2 version="2"/>
    </projection>
  </postprocessing>

</simulation>
